{
  "name": "SickBeard",
  "version": "latest",
  "slug": "sickbeard_docker",
  "description": "The ultimate PVR application that searches for and manages your TV shows Automatically finds new and old episodes for you and it works with your current download client!",
  "url": "https://github.com/NilSkilz/Hassio-Addons/nzbget",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "ports": {
    "8081/tcp": 8081
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8081]",
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {}
}
